{
  "manifest_version": 3,
  "name": "2048 Reloaded",
  "version": "1.1",
  "description": "Play 2048 in fullscreen with ads!",
  "action": {
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
